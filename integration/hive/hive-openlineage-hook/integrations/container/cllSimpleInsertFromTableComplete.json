      "jobType": {
        "integration": "HIVE",
        "jobType": "QUERY",
        "processingType": "BATCH"
      },
      "sql": {
        "dialect": "hive",
        "query": "INSERT INTO t1 SELECT a, concat(b, 'x') FROM t2"
      }
    }
  },
  "inputs": [
    {
      "namespace": "hive://localhost:9083",
      "name": "test.t2",
      "facets": {
