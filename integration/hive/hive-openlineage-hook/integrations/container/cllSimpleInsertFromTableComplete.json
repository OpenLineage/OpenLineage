              "name": "a",
              "type": "int"
            },
            {
              "name": "b",
              "type": "string"
            }
          ]
        },
        "columnLineage": {
          "fields": {
            "a": {
              "inputFields": [
                {
                  "namespace": "hive://localhost:9083",
                  "name": "test.t2",
                  "field": "a",
                  "transformations": [
                    {
                      "type": "DIRECT",
                      "subtype": "IDENTITY",
                      "description": "",
                      "masking": false
                    }
                  ]
                }
              ]
            },
            "b": {
              "inputFields": [
                {
                  "namespace": "hive://localhost:9083",
                  "name": "test.t2",
